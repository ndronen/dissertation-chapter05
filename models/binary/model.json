{
    "non_word_csv": "../chapter04/data/train.csv",

    "model_input_width": 28,
    "non_word_input_name": "non_word_input",
    "real_word_input_name": "real_word_input",
    "target_name": "binary_correction_target",

    "n_embeddings": 255,
    "n_embed_dims": 100,
    "n_filters": 1000,
    "filter_width": 6,
    "n_fully_connected": 2,
    "n_residual_blocks": 10,
    "n_hidden": 200,

    "train_embeddings": true,
    "embedding_init": "uniform",
    "batch_normalization": true,

    "optimizer": "Adam",
    "loss": "categorical_crossentropy",
    "l2_penalty": 0.0,

    "dropout_embedding_p": 0.0,
    "dropout_conv_p": 0.0,
    "dropout_fc_p": 0.0,

    "patience": 1,

    "embedding_max_norm": 1000,
    "filter_max_norm": 1000,
    "dense_max_norm": 1000,
    "clipnorm": 0,
    "border_mode": "valid"
}
